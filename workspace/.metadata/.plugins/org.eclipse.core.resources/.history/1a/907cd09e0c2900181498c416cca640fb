package cn.itcast_01;

import java.awt.Frame;

/** 
* @author 作者 E-mail: 
* @version 创建时间：2018年3月16日 下午7:17:11 
* 类说明 
*/
public class FrameDemo {
	public static void main(String[] args) {
		//创建窗体对象
		//构造一个最初不可见的实例窗体
		Frame f = new Frame();
		//调用一个方法，使得窗体可见
		//f.show();
		//替代
		f.setVisible(true);
		
		//System.out.println("helloworld");
	}

}
