package cn.itcast_01;
/** 
* @author 作者 E-mail: 
* @version 创建时间：2018年3月20日 下午4:00:59 
* 类说明 
*需求：计算出一段代码的运行时间
*/
public class GetTime {
	public long getTime(){
		long start = System.currentTimeMillis();
		
		for(int x = 0 ;x<10000;x++){
			System.out.println(x);
		}
		long end = System.currentTimeMillis();
		return end - start;
		
	}

}
